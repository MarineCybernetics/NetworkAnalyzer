                <LineChart
                   data={data}
                   width={5000}
                   height={400}
                   margin={{top: MARGINS.top, bottom: MARGINS.bottom, left: MARGINS.left, right: MARGINS.right}}
                   xScale={xScale}
                   xAxis={{label: "Time:s", tickValues: xScale.ticks(200), tickPadding: 5}}
                   yAxis={{label: "Packets:bytes"}}
                   tooltipHtml={tooltipLine}
                   tooltipContained
                   shapeColor={"red"}/>